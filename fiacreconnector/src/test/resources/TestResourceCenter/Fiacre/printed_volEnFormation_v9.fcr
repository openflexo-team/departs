			if (g.synD.Reconf_Order_fromEIU and (id = D)) then
				g.varD.ChemProp_isON := false;
				t_ON := 0;
				g.varD.ChemProp_redundance := (g.varD.ChemProp_redundance - 1);
				g.synD.ChemProp_Failed := true;

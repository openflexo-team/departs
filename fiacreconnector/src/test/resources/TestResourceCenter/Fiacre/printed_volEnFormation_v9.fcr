init event := {val_signal = 0, type_signal = 0};
firstAcq := true;
/* ------------------------------------------------
              
-------------------------------------------------*/
to StandBy

from StandBy
	if ((g.varD.CmdCtrl_mode = 1) and (g.varD.Payload_mode = 1)) then
		g.varD.SatSafeMode := 0;

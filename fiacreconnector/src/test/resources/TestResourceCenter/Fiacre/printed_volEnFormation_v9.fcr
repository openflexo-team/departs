	else 
		/* ------------------------------------------------
              
-------------------------------------------------*/
		to StandBy
	end if

from Init
	if (((g.varD.CmdCtrl_mode = 1) and (g.varD.Payload_mode = 1)) and firstAcq) then
		firstAcq := false;
		to Launch
	else 
		if g.synD.ARO then
			to StandBy
		else 
			if ((((firstAcq = false) and (g.varD.GNC_mode = 1)) and (g.varD.CmdCtrl_mode = 1)) and (g.varD.Payload_mode = 1)) then
				g.varD.SatSafeMode := 1;

		to CollisionAvoidance
	else 
		if g.synD.ARO then
			to StandBy
		else 
			if (((g.varD.GNC_mode = 4) and (g.varD.CmdCtrl_mode = 2)) and (g.varD.Payload_mode = 2)) then
				to Operational
			else 
				/* ------------- Fin Satellite_DSC ----------------------*/
				/*--------------------Fin--------------------*/
				/*=============================================
			Satellite_MSC
===============================================*/
				/*--------------------Debut--------------------*/
				to Suspend
			end if
		end if
	end if



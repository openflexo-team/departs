				end if
			end if
		end if
	end if

from Failed
	if (((t_ON >= 900) and (g.varD.ChemProp_redundance > 0)) and (id = D)) then
		restore := 1;
		g.varD.ChemProp_isON := false;
		to OFF
	else 
		if (((t_ON >= 900) and (g.varM.ChemProp_redundance > 0)) and (id = M)) then
			restore := 1;

from Launch
	if (((g.varD.GNC_mode = 1) and (g.varD.CmdCtrl_mode = 1)) and (g.varD.Payload_mode = 1)) then
		g.varD.SatSafeMode := 1;
		to Safe
	else 
		if g.synD.ARO then
			to StandBy
		else 

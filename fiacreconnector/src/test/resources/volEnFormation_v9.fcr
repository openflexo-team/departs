		end;
		if (g.synD.RF_PFail) then
			g.varD.RF_mode := 4;
			redundance := redundance - 1;
			t_ON := 0;
			g.synD.Reconf_Order_toEIU := true;
			g.synD.RF_Failed := true ;
			to Failed
		end
	end;

to Fine
/* ------------------------------------------------
              
-------------------------------------------------*/
from Failed

	if (t_ON >= 400 and redundance > 0 and id = D) then
		restore := 1;
		g.varD.RF_mode := 0;
		t_ON := 0;
		to OFF
	end;
	
	if (t_ON >= 400 and redundance > 0 and id = M) then
		restore := 1;
		g.varM.RF_mode := 0;
		t_ON := 0;
		to OFF
	end;
	
to Failed


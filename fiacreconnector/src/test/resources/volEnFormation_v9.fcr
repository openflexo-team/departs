init 
	id := 0 ;
	received :=
	{
		type_signal = NO_SIGNAL,
		val_signal = 0
	};
	to waitSignal

	
from waitSignal

	while (not (empty to_bus) and received.type_signal = NO_SIGNAL)
	do
		received := first to_bus;
		to_bus := dequeue (to_bus)
	end;
	if (received.type_signal = TC_CmdCtrl_toFormation) then
		to sendTC_CmdCtrl_toFormation
	end;
	if (received.type_signal = Ampli_PFail) then
		to sendAmpli_PFail
	end;
	if (received.type_signal = Ampli_TFail) then
		to sendAmpli_TFail
	end;
	if (received.type_signal = Battery_FailDischarged) then
		to sendBattery_FailDischarged
	end;
	if (received.type_signal = Battery_FailParamConsist) then
		to sendBattery_FailParamConsist
	end;
	if (received.type_signal = IMU_PFail) then
		to sendIMU_PFail
	end;
	if (received.type_signal = IMU_TFail) then
		to sendIMU_TFail
	end;
	if (received.type_signal = OBC_PFail) then
		to sendOBC_PFail
	end;
	if (received.type_signal = OBC_TFail) then
		to sendOBC_TFail
	end;
	if (received.type_signal = PCDU_PFail) then
		to sendPCDU_PFail
	end;
	if (received.type_signal = PCDU_TFail) then
		to sendPCDU_TFail
	end;
	if (received.type_signal = SAS_PFail) then
		to sendSAS_PFail
	end;
	if (received.type_signal = SAS_TFail) then
		to sendSAS_TFail
	end;
	if (received.type_signal = STR_PFail) then
		to sendSTR_PFail
	end;
	if (received.type_signal = STR_TFail) then
		to sendSTR_TFail
	end;
	if (received.type_signal = System_Fail) then
		to sendSystem_Fail
	end;
	if (received.type_signal = TCM_GNC_toACM) then
		to sendTCM_GNC_toACM
	end;
	if (received.type_signal = TCM_GNC_toOCM) then
		to sendTCM_GNC_toOCM
	end;
	if (received.type_signal = TC_EIU_ON) then
		to sendTC_EIU_ON
	end;
	if (received.type_signal = TC_OBC_ON) then
		to sendTC_OBC_ON
	end;
	if (received.type_signal = TC_PL_toOp) then
		to sendTC_PL_toOp
	end;
	if (received.type_signal = Transponder_PFail) then
		to sendTransponder_PFail
	end;
	if (received.type_signal = Transponder_TFail) then
		to sendTransponder_TFail
	end;

	
to waitSignal


from sendTC_CmdCtrl_toFormation

		BUS_IMU ! received;
		received.type_signal := NO_SIGNAL;

to waitSignal

from sendTC_OBC_ON

		BUS_GNC ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal

from sendOBC_PFail

		BUS_GNC ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal

from sendOBC_TFail
	
		BUS_GNC ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal
	
from sendAmpli_PFail

		BUS_OBC ! received;
		received.type_signal := NO_SIGNAL;

to waitSignal

from sendAmpli_TFail

		BUS_OBC ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal

from sendBattery_FailDischarged

		BUS_Ampli ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal

from sendBattery_FailParamConsist

		BUS_Ampli ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal
	
from sendTCM_GNC_toACM

		BUS_Battery ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal
	
from sendTCM_GNC_toOCM

		BUS_GNC ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal
		
from sendTC_EIU_ON

		BUS_EIU ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal
		
from sendTC_PL_toOp

		BUS_Payload ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal
	
from sendPCDU_PFail

		BUS_PCDU ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal

from sendPCDU_TFail

		BUS_PCDU ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal

from sendSAS_PFail
	
		BUS_SAS ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal

from sendSAS_TFail

		BUS_SAS ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal
	
from sendSTR_PFail

		BUS_STR ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal

from sendSTR_TFail

		BUS_STR ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal
		
from sendTransponder_PFail

		BUS_Transponder ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal

from sendTransponder_TFail

		BUS_Transponder ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal
		
from sendSystem_Fail

		BUS_System_Surveillance ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal
		
from sendIMU_PFail

		BUS_IMU ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal

from sendIMU_TFail

		BUS_IMU ! received;
		received.type_signal := NO_SIGNAL;
		
to waitSignal




/* ------------- Fin BUS ----------------------*/
/*--------------------Fin--------------------*/
/*=============================================
            Composant a valider
===============================================*/
/*--------------------Debut--------------------*/
component main is

var	
	g : STRUCT_GLOBAL,
	ToBus  : fifo,	
	ToContext  : fifo	

port
	BUS_CmdCtrl : SIGNAL,
	BUS_OBC : SIGNAL,
	BUS_Battery : SIGNAL,
	BUS_Ampli : SIGNAL,
	BUS_EIU : SIGNAL,
	
	BUS_ISL : SIGNAL,
	BUS_IMU : SIGNAL,
	BUS_PCDU : SIGNAL,
	BUS_Payload : SIGNAL,
	BUS_SAS : SIGNAL,
	BUS_STR : SIGNAL,
	BUS_System_Surveillance : SIGNAL,
	BUS_Transponder : SIGNAL,
	BUS_GNC : SIGNAL,
	ISL_BUS : SIGNAL
	





/* initialisations des variables du composant  */	
/*--------------------Debut--------------------*/
init

	ToBus := {||};
	ToContext := {||};
		
	g :=
	{		
		synD =
		{

			/* Broadcast */

			GNC_ASM_ON = false,
			GNC_ACM_ON = false,
			GNC_OCM_ON = false,
			GNC_FCM_ON = false,
			ARO = false,
			CAM = false,
			CmdCtrl_Auto_ON	= false,
			Transponder_Failed	= false,
			Ampli_Failed	= false,
			ChemProp_Failed	= false,
			SAS_Failed	= false,
			IMU_Failed	= false,
			STR_Failed	= false,
			GasProp_Failed	= false,
			RF_Failed	= false,
			LatSens_Failed	= false,
			FDIR_ToPLSafe = false,
			Reconf_Order_fromTranspondeur = false,
			Reconf_Order_fromEIU = false,
			Reconf_Order_toEIU = false,
			Reconf_Order_fromAmpli = false,
			LatSensSurveillance_Detected = false,
			TC_GNC_toFCM = false,
			ChemProp_TFail = false,
			ChemProp_PFail = false,
			GasProp_TFail = false,
			GasProp_PFail = false,
			RF_TFail = false,
			RF_PFail = false,
			EIU_TFail = false,
			EIU_PFail = false,
			LatSens_TFail = false,
			LatSens_PFail = false
		},
		
		synM =
		{

			/* Broadcast */

			GNC_ASM_ON = false,
			GNC_ACM_ON = false,
			GNC_OCM_ON = false,
			GNC_FCM_ON = false,
			ARO = false,
			CAM = false,
			CmdCtrl_Auto_ON	= false,
			Transponder_Failed	= false,
			Ampli_Failed	= false,
			ChemProp_Failed	= false,
			SAS_Failed	= false,
			IMU_Failed	= false,
			STR_Failed	= false,
			GasProp_Failed	= false,
			RF_Failed	= false,
			LatSens_Failed	= false,
			FDIR_ToPLSafe = false,
			Reconf_Order_fromTranspondeur = false,
			Reconf_Order_fromEIU = false,
			Reconf_Order_toEIU = false,
			Reconf_Order_fromAmpli = false,
			LatSensSurveillance_Detected = false,
			TC_GNC_toFCM = false,
			ChemProp_TFail = false,
			ChemProp_PFail = false,
			GasProp_TFail = false,
			GasProp_PFail = false,
			RF_TFail = false,
			RF_PFail = false,
			EIU_TFail = false,
			EIU_PFail = false,
			LatSens_TFail = false,
			LatSens_PFail = false
		},
		
		varD = 
		{	
			id = D,
			
			/* need global var for redundancies of ChemProp, GasProp, 
			EIU for visibility to Relative Position Surveillance */
			
			EIU_redundance = 2,
			ChemProp_redundance = 2,
			GasProp_redundance = 2,
			GNC_mode = 0,
			GNC_modeDelayed = 0,
			CmdCtrl_mode = 0,
			CmdCtrl_modeDelayed = 0,
			Payload_mode = 0,
			Payload_modeDelayed = 0,
			RF_mode = 0,
			RF_modeDelayed = 0,
			Formation_mode = 0,
			
			/* check equipments are ON */
			
			ChemProp_isON = false,
			IMU_isON = false,
			STR_isON = false,
			SAS_isON = true,   
			OBC_isON = true,
			Battery_isON = true,
			EIU_isON = true,
			PCDU_isON = true,
			GasProp_isON = false,
			LatSens_isON = false,
			Transponder_isON = false,
			Ampli_isON = false,
			
			/* check Sat in safe mode */
			
			SatSafeMode = 0,
			SatSafeModeDelayed = 0,

			/* notify equipment failures to trigger transition to degraded mode */
			
			
			RFSurveillance_Detected = false,


			/* formation or not (necessary to know if the TCs are going through the ISL) */
			
			CmdCtrl_isFormation = false,

			/* for surveillances --> notify failure */
			
			EIU_isFailed = false,
			
			/* avoid sending twice the same TC
			(possible since we have intermediate modes that are not numbered) */
			
			GNC_toACM_engaged = false,
			GNC_toOCM_engaged = false,
			GNC_toFCM_engaged = false
		},
		
		varM = 
		{	
			id = M,
			
			/* need global var for redundancies of ChemProp, GasProp, 
			EIU for visibility to Relative Position Surveillance */
			
			EIU_redundance = 2,
			ChemProp_redundance = 2,
			GasProp_redundance = 2,
			GNC_mode = 0,
			GNC_modeDelayed = 0,
			CmdCtrl_mode = 0,
			CmdCtrl_modeDelayed = 0,
			Payload_mode = 0,
			Payload_modeDelayed = 0,
			RF_mode = 0,
			RF_modeDelayed = 0,
			Formation_mode = 0,
			
			/* check equipments are ON */
			
			ChemProp_isON = false,
			IMU_isON = false,
			STR_isON = false,
			SAS_isON = true,
			OBC_isON = true,
			Battery_isON = true,
			EIU_isON = true,
			PCDU_isON = true,
			GasProp_isON = false,
			LatSens_isON = false,
			Transponder_isON = false,
			Ampli_isON = false,
			
			/* check Sat in safe mode */
			
			SatSafeMode = 0,
			SatSafeModeDelayed = 0,

			/* notify equipment failures to trigger transition to degraded mode */
			
			
			RFSurveillance_Detected = false,


			/* formation or not (necessary to know if the TCs are going through the ISL) */
			
			CmdCtrl_isFormation = false,

			/* for surveillances --> notify failure */
			
			EIU_isFailed = false,
			
			/* avoid sending twice the same TC
			(possible since we have intermediate modes that are not numbered) */
			
			GNC_toACM_engaged = false,
			GNC_toOCM_engaged = false,
			GNC_toFCM_engaged = false
		}
	}
	
	
/*--------------------Fin--------------------*/
par

		BUS_CmdCtrl 				-> CmdCtrl [BUS_CmdCtrl](&g, D)
	||	BUS_CmdCtrl 				-> CmdCtrl [BUS_CmdCtrl](&g, M)
	
	||	BUS_OBC 					-> OBC [BUS_OBC](&g, D)
	||	BUS_OBC 					-> OBC [BUS_OBC](&g, M)
	
	||	BUS_Ampli					-> Ampli [BUS_Ampli](&g, D)
	||	BUS_Ampli					-> Ampli [BUS_Ampli](&g, M)
	
	||	BUS_Battery					-> Battery [BUS_Battery](&g, D)
	||	BUS_Battery					-> Battery [BUS_Battery](&g, M)
	
	||	ChemProp (&g, D)
	||	ChemProp (&g, M)
	
	||	BUS_EIU						-> EIU [BUS_EIU](&g, D)
	||	BUS_EIU						-> EIU [BUS_EIU](&g, M)
	
	||	Formation (&g, D)
	||	Formation (&g, M)
	
	||	BUS_GNC						-> GNC [BUS_GNC](&g, D)	
	||	BUS_GNC						-> GNC [BUS_GNC](&g, M)
	
	||	GasProp (&g, D)
	
	||	BUS_IMU						-> IMU [BUS_IMU](&g, D)
	||	BUS_IMU						-> IMU [BUS_IMU](&g, M)
	
	||	BUS_ISL,ISL_BUS			-> ISL [BUS_ISL,ISL_BUS](&g)
	
	||	LatSensSurveillance (&g)
	
	||	LatSens_DSC (&g, D)
	
	||	BUS_PCDU					-> PCDU [BUS_PCDU](&g, D)
	||	BUS_PCDU					-> PCDU [BUS_PCDU](&g, M)
	
	||	BUS_Payload					-> Payload [BUS_Payload](&g)
	
	||	ProximitySurveillance (&g, D)
	||	ProximitySurveillance (&g, M)
	
	||	RF (&g, D)
	||	RF (&g, M)
	
	||	RF_Surveillance (&g)
	
	||	RelPosSurveillance_DSC (&g)
	||	RelPosSurveillance_MSC (&g)
	
	||	BUS_SAS						-> SAS [BUS_SAS](&g, D)
	||	BUS_SAS						-> SAS [BUS_SAS](&g, M)

	||	BUS_STR						-> STR [BUS_STR](&g, D)
	||	BUS_STR						-> STR [BUS_STR](&g, M)
	
	||	Satellite_DSC (&g)
	||	Satellite_MSC (&g)
	
	||	BUS_System_Surveillance		-> System_Surveillance [BUS_System_Surveillance](&g, D)
	||	BUS_System_Surveillance		-> System_Surveillance [BUS_System_Surveillance](&g, M)
	
	||	BUS_Transponder				-> Transponder [BUS_Transponder](&g, D)
	||	BUS_Transponder				-> Transponder [BUS_Transponder](&g, M)
	
	||	BUS_CmdCtrl,BUS_OBC,
		BUS_Battery,BUS_Ampli,BUS_GNC,
		BUS_ISL,BUS_STR,BUS_PCDU,
		BUS_SAS,ISL_BUS,BUS_Payload,
		BUS_IMU,BUS_EIU,
		BUS_System_Surveillance,
		BUS_Transponder			-> BUS	[BUS_CmdCtrl,BUS_OBC,
											BUS_Battery,BUS_Ampli,BUS_GNC,
											BUS_ISL,BUS_STR,BUS_PCDU,
											BUS_SAS,ISL_BUS,BUS_Payload,
											BUS_IMU,BUS_EIU,
											BUS_System_Surveillance,
											BUS_Transponder](&g,&ToContext, &ToBus)
	
	||	Context (&ToContext, &ToBus)
	
	
end

main
/*--------------------Fin--------------------*/


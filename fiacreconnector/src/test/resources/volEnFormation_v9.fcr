		g.varM.ChemProp_redundance := g.varM.ChemProp_redundance - 1;
		g.synM.ChemProp_Failed := true;
		to Failed
	end;
		
	
	if (id = D and g.synD.ChemProp_TFail) then
		g.varD.ChemProp_isON := false;
		t_ON := 0;

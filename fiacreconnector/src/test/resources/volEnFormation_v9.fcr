/* je garde volontairement la structure en 
variable globale pour ne pas trop augmenter mon nombre de variable et d'états */

process OBC [BUS_OBC : in SIGNAL ](&g : read write STRUCT_GLOBAL, id : int)  is

states 
	ON,
	Starting,
	OFF,
	Committed,
	Failed

var
	restore : int,
	t_ON : int,
	event : SIGNAL,
	redundance : int
	
init
	restore := 0;
	t_ON := 0;
	event := 
	{
		val_signal = 0,
		type_signal = 0
	};
	redundance := 0;
	
	
to ON

/* ------------------------------------------------
              
-------------------------------------------------*/
from ON
		
	if (g.synD.ARO = true and id = D) then
		g.varD.OBC_isON := false;
		to OFF
	end;
	if (g.synM.ARO = true and id = M) then
		g.varM.OBC_isON := false;
		to OFF
	end;
	
	if ( true ) then
		BUS_OBC ? event;
		if (event.val_signal = id and event.type_signal = OBC_TFail) then
			t_ON :=0;
			to Committed
		end;
		if (event.val_signal = id and event.type_signal = OBC_PFail) then
			t_ON :=0;
			redundance := redundance - 1;
			to Committed
		end
	end;
		
	
	
to ON	
/* ------------------------------------------------
              
-------------------------------------------------*/
from Committed


	if (id = D and event.val_signal = D) then
		g.varD.OBC_isON := false;
		t_ON := 0;
		g.synD.ARO := true;
		to Failed
	end;
	if (id = M and event.val_signal = M) then
		g.varM.OBC_isON := false;
		t_ON := 0;
		g.synM.ARO := true;
		to Failed
	end;
	
	
to Committed

/* ------------------------------------------------
              
-------------------------------------------------*/
from Starting
	
	if (t_ON >= 30 and id = D) then
		g.varD.OBC_isON := false;
		to ON
	end;
	if (t_ON >= 30 and id = M) then
		g.varM.OBC_isON := false;
		to ON
	end;
	
	if (g.synD.ARO = true and id = D) then
		to OFF
	end;
	if (g.synM.ARO = true and id = M) then
		to OFF
	end;
	
to Starting	
/* ------------------------------------------------
              
-------------------------------------------------*/
from OFF

	if (restore = 1) then
		to Starting
	end;
	if ( true ) then
		BUS_OBC ? event;
		if (event.val_signal = id and event.type_signal = TC_OBC_ON) then
			t_ON :=0;
			restore := 0;
			to Starting
		end
	end;
	
	
to OFF
/* ------------------------------------------------
              
-------------------------------------------------*/
from Failed

	if (t_ON >= 10 and redundance>0) then
		restore := 1;
		to OFF
	end;
	
to Failed	
/* ------------- Fin OBC ----------------------*/

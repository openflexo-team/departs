process Satellite_DSC (&g : read write STRUCT_GLOBAL)  is

states 
	StandBy,
	Init,
	Launch,
	Safe,
	Normal,
	CollisionAvoidance,
	Suspend,
	Operational

var
	event : SIGNAL,
	firstAcq : bool
	
init
	event := 
	{
		val_signal = 0,
		type_signal = 0
	};
	firstAcq := true;
	
	
to StandBy
/* ------------------------------------------------
              
-------------------------------------------------*/
from StandBy

	if (g.varD.CmdCtrl_mode = 1 and g.varD.Payload_mode = 1) then
		g.varD.SatSafeMode := 0;
		to Init
	end;
	
to StandBy	
/* ------------------------------------------------
              
-------------------------------------------------*/
from Init
	
	if (g.varD.CmdCtrl_mode = 1 and g.varD.Payload_mode = 1 and firstAcq) then
		firstAcq := false;
		to Launch
	end;
	if (g.synD.ARO) then
		to StandBy
	end;
	if (firstAcq = false and g.varD.GNC_mode = 1 and g.varD.CmdCtrl_mode = 1 and g.varD.Payload_mode = 1) then
		g.varD.SatSafeMode := 1;
		to Safe
	end;
	
to Init
/* ------------------------------------------------
              
-------------------------------------------------*/
from Launch

	if (g.varD.GNC_mode = 1 and g.varD.CmdCtrl_mode = 1 and g.varD.Payload_mode = 1) then
		g.varD.SatSafeMode := 1;
		to Safe
	end;
	if (g.synD.ARO) then
		to StandBy
	end;
to Launch
/* ------------------------------------------------
              
-------------------------------------------------*/
from Safe
	
	if ((g.varD.GNC_mode = 2 or g.varD.GNC_mode = 3) and (g.varD.CmdCtrl_mode = 1 or g.varD.CmdCtrl_mode = 2) and g.varD.Payload_mode = 1) then
		g.varD.SatSafeMode := 2;
		to Normal
	end;
	if (g.synD.ARO) then
		to StandBy
	end;
	
to Safe
/* ------------------------------------------------
              
-------------------------------------------------*/
from Normal

	if (g.synD.CAM) then
		to CollisionAvoidance
	end;
	
	if (g.synD.ARO) then
		to StandBy
	end;
	
	if (g.varD.GNC_mode = 4 and g.varD.CmdCtrl_mode = 2 and g.varD.Payload_mode = 2) then
		to Operational
	end;
	
to Normal
/* ------------------------------------------------
              
-------------------------------------------------*/
from CollisionAvoidance

	if ((g.varD.GNC_mode = 2 or g.varD.GNC_mode = 3) and (g.varD.CmdCtrl_mode = 1 or g.varD.CmdCtrl_mode = 2) and g.varD.Payload_mode = 1) then
		to Normal
	end;
	if (g.synD.ARO) then
		to StandBy
	end;
	
to CollisionAvoidance
/* ------------------------------------------------
              
-------------------------------------------------*/
from Operational

	if ((g.varD.GNC_mode = 2 or g.varD.GNC_mode = 3) and (g.varD.CmdCtrl_mode = 1 or g.varD.CmdCtrl_mode = 2) and g.varD.Payload_mode = 1) then
		to Normal
	end;
	if (g.synD.CAM) then
		to CollisionAvoidance
	end;
	if (g.synD.ARO) then
		to StandBy
	end;
	
	if (g.synD.LatSensSurveillance_Detected or g.synD.FDIR_ToPLSafe or g.varD.RFSurveillance_Detected) then
		to Suspend
	end;
	
to Operational
/* ------------------------------------------------
              
-------------------------------------------------*/
from Suspend
	
	if (g.synD.CAM) then
		to CollisionAvoidance
	end;
	if (g.synD.ARO) then
		to StandBy
	end;
	if (g.varD.GNC_mode = 4 and g.varD.CmdCtrl_mode = 2 and g.varD.Payload_mode = 2) then
		to Operational
	end;

to Suspend
